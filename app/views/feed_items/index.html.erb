<p id="notice"><%= notice %></p>

<h1>Feed Items</h1>

<table>
  <thead>
    <tr>
      <th>Item Type</th>
      <th>Username</th>
      <th>User</th>
      <th>Item description</th>
      <th>Image url</th>
      <th>Video url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feed_items.each do |feed_item| %>
      <tr>
        <td><%= feed_item.item_type %></td>
        <td><%= feed_item.username %></td>
        <td><%= feed_item.user_id %></td>
        <td><%= feed_item.item_description %></td>
        <td><%= feed_item.image_url %></td>
        <td><%= feed_item.video_url %></td>
        <td><%= link_to 'Show', feed_item %></td>
        <td><%= link_to 'Edit', edit_feed_item_path(feed_item) %></td>
        <td><%= link_to 'Destroy', feed_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feed Item', new_feed_item_path %>
