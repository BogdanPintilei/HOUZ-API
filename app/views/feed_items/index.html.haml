%p#notice= notice
%h1 Feed Items
%table
  %thead
    %tr
      %th Item Type
      %th Username
      %th User
      %th Item description
      %th Image url
      %th Video url
      %th{:colspan => "3"}
  %tbody
    - @feed_items.each do |feed_item|
      %tr
        %td= feed_item.item_type
        %td= feed_item.username
        %td= feed_item.user_id
        %td= feed_item.item_description
        %td= feed_item.image_url
        %td= feed_item.video_url
        %td= link_to 'Show', feed_item
        %td= link_to 'Edit', edit_feed_item_path(feed_item)
        %td= link_to 'Destroy', feed_item, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Feed Item', new_feed_item_path
